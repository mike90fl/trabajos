Console.WriteLine("Escribe tu Primer Numero: ");
int num1 = Convert.ToInt32(Console.ReadLine());



Console.WriteLine("Escribe tu segundo numero :D");
int num2 = Convert.ToInt32(Console.ReadLine());

if(num1 >= num2){​​



Console.WriteLine("Primer Numero es mayor o igual al segundo");
}​​



else {​​



Console.WriteLine("El segundo numero es mayor al primero");



}​​


